<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:dpi="urn:oecd:ties:dpi:v1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:stf="urn:oecd:ties:dpistf:v1" xmlns:iso="urn:oecd:ties:isodpitypes:v1" targetNamespace="urn:oecd:ties:dpi:v1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.09">
	<xsd:import namespace="urn:oecd:ties:isodpitypes:v1" schemaLocation="isodpitypes_v1.0.xsd"/>
	<xsd:import namespace="urn:oecd:ties:dpistf:v1" schemaLocation="oecddpitypes_v1.0.xsd"/>
	<!--+++++++++++++++++++++++  Újrafelhasználható egyszerű típusok / Reusable Simple types ++++++++++++++++++++++++++++++++++++++ -->
	<!-- Message típus definíciók / Message type definitions -->
	<xsd:simpleType name="MessageType_EnumType">
		<xsd:annotation>
			<xsd:documentation xml:lang="hu">A MessageType kötelező elem jelzi az adatszolgáltatás típusát</xsd:documentation>
			<xsd:documentation xml:lang="en">Message type mandatory element defines the type of reporting.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DPI">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">Ez a típus jelzi a 2013. évi XXXVII. törvény (Aktv.) alapján az Adatszolgáltatásra kötelezett platformüzemeltetők adatszolgáltatásához kapcsolódó üzeneteket.</xsd:documentation>
					<xsd:documentation xml:lang="en">This type defines the messages to be reported by the Reporting Platform Operators under Act XXXVII of 2013 (Aktv.).</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- MessageTypeIndic - 4 -->
	<xsd:simpleType name="DPIMessageTypeIndic_EnumType">
		<xsd:annotation>
			<xsd:documentation xml:lang="hu">A MessageTypeIndic kötelező elem jelzi a küldött üzenet jellegét.</xsd:documentation>
			<xsd:documentation xml:lang="en">The MessageTypeIndic mandatory element defines the type of message sent.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DPI401">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">Az üzenet új (esetlegesen kiegészítő) adatokat tartalmaz.</xsd:documentation>
					<xsd:documentation xml:lang="en">The message contains new (including additional) information.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DPI402">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">Az üzenet valamely korábban küldött adatot érintő helyesbítést tartalmaz.</xsd:documentation>
					<xsd:documentation xml:lang="en">The message contains corrections for previously sent information.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DPI403">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">Az üzenet azt jelzi, hogy az Adatszolgáltatásra kötelezett platformüzemeltető az átvilágítási eljárás eredményeként nem azonosított jelentendő adatot az adatszolgáltatási időszakot érintően (Aktv. 21/B. § (2) bekezdés), vagy azonosított, de mentesül az adatok jelentése alól, mivel ugyanazon információkat egy másik Adatszolgáltatásra kötelezett platformüzemeltető már bejelentette (Aktv. 5. melléklet III/A/1-2. pontok második mondata).</xsd:documentation>
					<xsd:documentation xml:lang="en">The message advises that the Reporting Platform Operator did not identify any reportable information through its due diligence procedures for the reportable period (Aktv. Section 21/B. paragraph 2), or that it identified such information however is exempt from reporting, because the same information has been reported by another Reporting Platform Operator (second sentence of Section III/A/1 and 2 of Annex 5 of the Aktv.)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!--  -->
	<xsd:simpleType name="INType_EnumType">
		<xsd:annotation>
			<xsd:documentation xml:lang="hu">Az INType jelzi a feltüntetett azonosító szám típusát.</xsd:documentation>
			<xsd:documentation xml:lang="en">The INType defines the type of identification number being sent.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LEI">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">jogi személy azonosító (Legal Entity Identifier)</xsd:documentation>
					<xsd:documentation xml:lang="en">Legal Entity Identifier</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="EIN">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">szervezet azonosító szám (Entity Identification Number)</xsd:documentation>
					<xsd:documentation xml:lang="en">Entity Identification Number</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IIN">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu"> Az Aktv. 21/A. § (5) bekezdése szerinti egyedi azonosító szám (Individual Identification Number)</xsd:documentation>
					<xsd:documentation xml:lang="en">Individual Identification Number pursuant to paragraph 5 of Section 21/A. of the Aktv.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BRN">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">cégnyilvántartási szám (Business Registration Number)</xsd:documentation>
					<xsd:documentation xml:lang="en">Business Registration Number</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation xml:lang="hu">egyéb</xsd:documentation>
					<xsd:documentation xml:lang="en">Other</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!--  -->
	<!--DPI ingatlan típus / DPI Property Type -->
